    <!--Begin Msg Number 8-->
		<tr>
			<td bgcolor="#444444">
			<table width=100% cellpadding=4 cellspacing=0 bgcolor="#444444">
			<tr>
        		<td bgcolor="#444444" valign="top" width=20%  rowspan=2>
            		<font face="arial" color="#FFC956" size="2">
            		<b>TheDPQ <img src="http://www.eyeball-design.com/ikonboard/images/team.gif" border=0></b></font>
            		<br>
            		<br><img src="http://www.eyeball-design.com/ikonboard/images/pips4.gif" border="0">
                    <br><font face="arial" color=#FFFFFFF size=1>Moderator</font>
