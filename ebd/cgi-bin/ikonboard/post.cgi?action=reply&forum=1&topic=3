clckcnt = 0;
function clckcntr() {
clckcnt++;
if(clckcnt > 1) {
    if(clckcnt > 2) { return false; }
    alert('Processing...\n\n' + '     Thank you for your patience.');
    return false;}
return true;}

</script>









